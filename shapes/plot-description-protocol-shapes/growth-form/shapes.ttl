    dcterms:source "TERN Ecosystem Surveillance Ecological Monitoring Protocols" ;
    reg:status reg:statusSubmitted ;
    sh:description "The value in `sosa:hasResult` _MUST_ be a value in `sh:in`, which is the Vegetation growth form codes controlled vocabulary." ;
    sh:in (
            <https://linked.data.gov.au/def/test/dawe-cv/062132e9-426e-56c8-9c92-2781692bdd0c>
            <https://linked.data.gov.au/def/test/dawe-cv/0c0c23e2-47c1-56c7-8fa0-3ac241caf196>
