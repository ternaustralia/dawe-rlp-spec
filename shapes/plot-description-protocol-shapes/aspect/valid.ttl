            a tern:FeatureOfInterest ;
            void:inDataset <urn:test:dataset> ;
            tern:featureType <http://linked.data.gov.au/def/tern-cv/2cf3ed29-440e-4a50-9bbc-5aab30df9fcd>
        ] ;
    sosa:hasResult [
            a tern:Float ;
            rdf:value 50.65 ;
            tern:unit unit:DEG
        ] ;
    sosa:hasSimpleResult 50.65 ;
    sosa:observedProperty <https://linked.data.gov.au/def/test/dawe-cv/0e0423c6-0dc3-40aa-9776-410a94299256> ;
    sosa:usedProcedure <https://linked.data.gov.au/def/test/dawe-cv/1ff9e97c-3bdd-44c9-bdd3-401fa31c0b32> ;
    tern:hasSiteVisit <urn:test:site> ;
.

