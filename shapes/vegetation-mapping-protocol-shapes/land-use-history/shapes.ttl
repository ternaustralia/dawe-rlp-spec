    sh:message "The observation's `sosa:usedProcedure` _MUST_ have the value `https://linked.data.gov.au/def/nrm/15361f98-7669-410e-9b04-e9be069c7508`." ;
    sh:name "Used procedure" ;
    sh:path sosa:usedProcedure ;
    sh:target [
