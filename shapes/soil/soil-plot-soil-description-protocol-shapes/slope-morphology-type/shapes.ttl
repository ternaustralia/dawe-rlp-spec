    dcterms:source "TERN Ecosystem Surveillance Ecological Monitoring Protocols" ;
    reg:status reg:statusSubmitted ;
    sh:description "The value in `sosa:hasResult` _MUST_ be a value in `sh:in`, which is the Soils morphological types codes controlled vocabulary." ;
    sh:in (
            <https://linked.data.gov.au/def/nrm/0ee92eba-1ef7-57f6-bdbe-0bd188a464d6>
            <https://linked.data.gov.au/def/nrm/19e86a69-0f75-5df2-b373-8e3735acfc97>
            <https://linked.data.gov.au/def/nrm/2313c79b-764c-5b67-b0c7-28c184f29b1b>
            <https://linked.data.gov.au/def/nrm/26963a55-885f-5904-b233-5e8e0adb543b>
            <https://linked.data.gov.au/def/nrm/27107204-613f-5a02-8685-62fed8a44169>
            <https://linked.data.gov.au/def/nrm/5b64674f-c553-5422-b5a1-e97b4d380c98>
            <https://linked.data.gov.au/def/nrm/69f1cbe3-e234-588b-a822-d73ccc9c2eb0>
            <https://linked.data.gov.au/def/nrm/7842574c-3823-50fd-a89b-642e0c94070f>
            <https://linked.data.gov.au/def/nrm/c1509ff9-da41-5a7a-be72-9821c2626677>
            <https://linked.data.gov.au/def/nrm/c9a7e7dd-70d2-5f99-9add-15802c870129>
        ) ;
    sh:message "The value of `rdf:value` _MUST_ exist in the Soils morphological types codes controlled vocabulary." ;
    sh:name "Result value" ;
    sh:path rdf:value ;
    sh:target [
            a sh:SPARQLTarget ;
            sh:select """
        PREFIX tern: <https://w3id.org/tern/ontologies/tern/>
        PREFIX sosa: <http://www.w3.org/ns/sosa/>
        PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
        select ?this  {
            ?observation a tern:Observation ;
                sosa:observedProperty 
        <https://linked.data.gov.au/def/nrm/b023d177-2c9b-4d58-86e6-099b742fcf1c> ;
                sosa:hasResult ?this .
        }"""
        ] ;
    urnp:examples _:n76249da63298494cb7900f663e640cbab2 ;
    urnp:query """
        PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
        select ?values {
            values ?categoricalUUID {'50e0e5aa-3abf-4883-a713-245701efe314'}
            ?s skos:member ?values .
            filter (STRENDS(str(?s), ?categoricalUUID))
        }
        """ ;
    urnp:sparqlEndpoint "https://graphdb.tern.org.au/repositories/dawe_vocabs_core" ;
    urnp:validator "https://github.com/ternaustralia/dawe-rlp-spec/blob/main/shapes/soil/soil-plot-soil-description-protocol-shapes/slope-morphology-type/shapes.ttl"^^xsd:anyURI ;
