        ) ;
    sh:message "The value of `rdf:value` _MUST_ exist in the Soils gilgal proportions codes controlled vocabulary." ;
    sh:name "Result value" ;
    sh:path rdf:value ;
    sh:target [
            a sh:SPARQLTarget ;
            sh:select """
        PREFIX tern: <https://w3id.org/tern/ontologies/tern/>
