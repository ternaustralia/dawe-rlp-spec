    sh:message "The observation's `sosa:usedProcedure` _MUST_ have the value `https://linked.data.gov.au/def/nrm/a2afccd5-766e-44bc-98c1-f27aae26727f`." ;
    sh:name "Used procedure" ;
    sh:path <http://www.w3.org/ns/sosa/usedProcedure> ;
