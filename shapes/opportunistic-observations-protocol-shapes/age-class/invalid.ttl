            a tern:FeatureOfInterest ;
            void:inDataset <https://example.com/dataset/1> ;
            tern:featureType <urn:fake:feature-type>
        ] ;
    sosa:hasResult [
            a
                tern:IRI ,
                tern:Value ;
            rdf:value <https://linked.data.gov.au/def/nrm/2befa1d5-33f2-5bc1-bc4c-0609b8dad9b9> ;
