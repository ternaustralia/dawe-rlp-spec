    dcterms:source "TERN Ecosystem Surveillance Ecological Monitoring Protocols" ;
    reg:status reg:statusSubmitted ;
    sh:description "The value in `sosa:hasResult` _MUST_ be a value in `sh:in`, which is the Vegetation type codes controlled vocabulary." ;
    sh:in (
            <https://linked.data.gov.au/def/nrm/024c3897-e700-5411-bfac-c84b48bf85f8>
            <https://linked.data.gov.au/def/nrm/0b3acbfe-9398-551b-b0aa-2483ad8200ff>
