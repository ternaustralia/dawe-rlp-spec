    sosa:resultTime "2022-09-27T05:38:47.117000+00:00"^^xsd:dateTime ;
    sosa:usedProcedure <https://linked.data.gov.au/def/nrm/37ed2dbb-b990-430c-9010-d0452588cf24> ;
