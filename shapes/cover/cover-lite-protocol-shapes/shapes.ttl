    reg:status reg:statusSubmitted ;
    sh:datatype xsd:string ;
    sh:description "The value in `sosa:hasResult` _MUST_ be xsd:string." ;
    sh:message "The value of `rdf:value` _MUST_ have the datatype xsd:string." ;
    sh:name "Datatype" ;
    sh:path rdf:value ;
