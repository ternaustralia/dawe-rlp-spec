    sh:message "The value of `tern:featureType` _MUST_ be link: http://linked.data.gov.au/def/tern-cv/e196c39e-959f-4dd4-8816-d0a58b7cc630[`weather events`]." ;
    sh:name "Feature type" ;
    sh:path tern:featureType ;
    sh:target [
