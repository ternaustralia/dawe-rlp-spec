            <https://linked.data.gov.au/def/nrm/277e5f6d-71e4-497e-bd8b-ac4d51aeaf66>
        ) ;
    sh:message "The value of `rdf:value` _MUST_ exist in the plant root abundances controlled vocabulary." ;
    sh:name "Result value" ;
    sh:path rdf:value ;
    sh:target [
            a sh:SPARQLTarget ;
            sh:select """
        PREFIX tern: <https://w3id.org/tern/ontologies/tern/>
        PREFIX sosa: <http://www.w3.org/ns/sosa/>
        PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
        select ?this  {
            ?observation a tern:Observation ;
                sosa:observedProperty 
        <https://linked.data.gov.au/def/nrm/cfd6aa2d-118a-497d-901c-29b10ce8dfae> ;
                sosa:hasResult ?this .
        }"""
        ] ;
    urnp:examples _:na423b3b8a5254719960d027f025c1723b2 ;
    urnp:query """
        PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
        select ?values {
            values ?categoricalUUID {'65ae0e16-e2a9-4b6c-9ac2-d68a499df23f'}
            ?s skos:member ?values .
            filter (STRENDS(str(?s), ?categoricalUUID))
        }
        """ ;
    urnp:sparqlEndpoint "https://graphdb.tern.org.au/repositories/dawe_vocabs_core" ;
    urnp:validator "https://github.com/ternaustralia/dawe-rlp-spec/blob/main/shapes/soils/soil-soil-site-observations-and-pit-characterisation-protocol-shapes/plant-root-abundance/shapes.ttl"^^xsd:anyURI ;
