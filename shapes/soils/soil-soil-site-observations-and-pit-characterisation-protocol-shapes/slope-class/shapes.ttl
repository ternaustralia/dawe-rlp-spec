        urnc:Requirement ;
    dcterms:source "Ecological Monitoring System - Australia Protocols" ;
    reg:status reg:statusSubmitted ;
    sh:description "The value in `sosa:hasResult` _MUST_ be a value in `sh:in`, which is the slope classes controlled vocabulary." ;
    sh:in (
            <https://linked.data.gov.au/def/nrm/43c0c157-aa65-5101-a0e9-f12e47e91cc1>
            <https://linked.data.gov.au/def/nrm/19775f85-619f-5bc2-84cf-db064f4083b4>
            <https://linked.data.gov.au/def/nrm/7ddc91f5-a5f0-5f3e-b18a-5b693d7100db>
