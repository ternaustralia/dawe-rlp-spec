    sh:message "The value of `rdf:value` _MUST_ exist in the Soil structure grades controlled vocabulary." ;
    sh:name "Result value" ;
    sh:path rdf:value ;
    sh:target [
            a sh:SPARQLTarget ;
            sh:select """
