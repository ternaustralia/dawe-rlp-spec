        ) ;
    sh:message "The value of `rdf:value` _MUST_ exist in the Soil horizon boundary shapes controlled vocabulary." ;
    sh:name "Result value" ;
    sh:path rdf:value ;
    sh:target [
            a sh:SPARQLTarget ;
            sh:select """
        PREFIX tern: <https://w3id.org/tern/ontologies/tern/>
        PREFIX sosa: <http://www.w3.org/ns/sosa/>
        PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
        select ?this  {
            ?observation a tern:Observation ;
                sosa:observedProperty 
        <https://linked.data.gov.au/def/nrm/ee03aeec-28ef-4910-94af-338a981bb40d> ;
                sosa:hasResult ?this .
        }"""
        ] ;
    urnp:examples _:n23ef7410550140dcaf4778f877bc9eb2b2 ;
    urnp:query """
        PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
        select ?values {
            values ?categoricalUUID {'2f514fb5-6c23-4fff-beed-858722a0f586'}
            ?s skos:member ?values .
            filter (STRENDS(str(?s), ?categoricalUUID))
        }
        """ ;
    urnp:sparqlEndpoint "https://graphdb.tern.org.au/repositories/dawe_vocabs_core" ;
    urnp:validator "https://github.com/ternaustralia/dawe-rlp-spec/blob/main/shapes/soils/soil-soil-site-observations-and-pit-characterisation-protocol-shapes/horizon-boundary-shape/shapes.ttl"^^xsd:anyURI ;
