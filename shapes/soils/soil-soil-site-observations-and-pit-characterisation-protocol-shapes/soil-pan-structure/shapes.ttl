    dcterms:source "Ecological Monitoring System - Australia Protocols" ;
    reg:status reg:statusSubmitted ;
    sh:description "The value in `sosa:hasResult` _MUST_ be a value in `sh:in`, which is the Soil pan structures controlled vocabulary." ;
    sh:in (
            <https://linked.data.gov.au/def/nrm/3fc85733-99bb-58c9-aa93-d79f4eb595ee>
            <https://linked.data.gov.au/def/nrm/94ef2c83-9b01-5170-83e6-c5427ff8d924>
