        sh:PropertyShape ,
        <urn:class:Requirement> ;
    dcterms:source "TERN Ecosystem Surveillance Ecological Monitoring Protocols" ;
    reg:status reg:statusSubmitted ;
    sh:description "Observations following the Plot Description protocol are made in the context of a site visit." ;
    sh:maxCount 1 ;
    sh:message "Instances of `tern:Observation` with `sosa:observedProperty` value `TBA` _MUST_ have a value for `tern:hasSiteVisit`." ;
